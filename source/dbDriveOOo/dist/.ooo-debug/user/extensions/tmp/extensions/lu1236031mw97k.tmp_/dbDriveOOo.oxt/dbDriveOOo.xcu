<?xml version='1.0' encoding='UTF-8'?>
<oor:component-data
  xml:lang="en-US"
  xmlns:oor="http://openoffice.org/2001/registry"
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  oor:package="org.openoffice.ucb"
  oor:name="Configuration">
    <node oor:name="ContentProviders">
        <node oor:name="Local">
            <node oor:name="SecondaryKeys">
                <node oor:name="Office">
                    <node oor:name="ProviderData">
                        <node oor:name="ProviderDropbox1" oor:op="fuse">
                            <prop oor:name="ServiceName">
                                <value>io.github.prrvchr.dbDriveOOo.ContentProvider</value>
                            </prop>
                            <prop oor:name="URLTemplate">
                                <value>"vnd-dropbox://"[^/?#]+"@outlook.com"([/?#].*)?</value>
                            </prop>
                            <prop oor:name="Arguments">
                                <value>WithAuthority</value>
                            </prop>
                        </node>
                        <node oor:name="ProviderDropbox2" oor:op="fuse">
                            <prop oor:name="ServiceName">
                                <value>io.github.prrvchr.dbDriveOOo.ContentProvider</value>
                            </prop>
                            <prop oor:name="URLTemplate">
                                <value>"vnd-dropbox://"[^/?#]+"@outlook.fr"([/?#].*)?</value>
                            </prop>
                            <prop oor:name="Arguments">
                                <value>WithAuthority</value>
                            </prop>
                        </node>
                        <node oor:name="ProviderDropbox3" oor:op="fuse">
                            <prop oor:name="ServiceName">
                                <value>io.github.prrvchr.dbDriveOOo.ContentProvider</value>
                            </prop>
                            <prop oor:name="URLTemplate">
                                <value>"vnd-dropbox://"[^/?#]+"@hotmail.com"([/?#].*)?</value>
                            </prop>
                            <prop oor:name="Arguments">
                                <value>WithAuthority</value>
                            </prop>
                        </node>
                        <node oor:name="ProviderDropbox4" oor:op="fuse">
                            <prop oor:name="ServiceName">
                                <value>io.github.prrvchr.dbDriveOOo.ContentProvider</value>
                            </prop>
                            <prop oor:name="URLTemplate">
                                <value>"vnd-dropbox://"[^/?#]+"@gmail.com"([/?#].*)?</value>
                            </prop>
                            <prop oor:name="Arguments">
                                <value>WithAuthority</value>
                            </prop>
                        </node>
                        <node oor:name="ProviderDropbox10" oor:op="fuse">
                            <prop oor:name="ServiceName">
                                <value>io.github.prrvchr.dbDriveOOo.ContentProvider</value>
                            </prop>
                            <prop oor:name="URLTemplate">
                                <value>"vnd-dropbox://"([/?#].*)?</value>
                            </prop>
                            <prop oor:name="Arguments">
                                <value>WithOutAuthority</value>
                            </prop>
                        </node>
                    </node>
                </node>
            </node>
        </node>
    </node>
</oor:component-data>